# MLflow - PostgreSQL (Tracking Database)
MLFLOW_POSTGRES_USER=mlflow
MLFLOW_POSTGRES_PASSWORD=change_me_mlflow_db_password
MLFLOW_POSTGRES_DB=mlflow

# MLflow - PostgreSQL (Auth Database)
MLFLOW_POSTGRES_AUTH_USER=mlflow_auth
MLFLOW_POSTGRES_AUTH_PASSWORD=change_me_mlflow_auth_db_password
MLFLOW_POSTGRES_AUTH_DB=mlflow_auth

# MLflow - MinIO (S3-compatible object storage)
MLFLOW_MINIO_ROOT_USER=minio
MLFLOW_MINIO_ROOT_PASSWORD=minio123
MLFLOW_MINIO_HOST=mlflow-minio
MLFLOW_MINIO_PORT=9000
MLFLOW_MINIO_BUCKET=mlflow

# MLflow - Server Configuration
MLFLOW_BACKEND_STORE_URI=postgresql://${MLFLOW_POSTGRES_USER}:${MLFLOW_POSTGRES_PASSWORD}@mlflow-postgres:5432/${MLFLOW_POSTGRES_DB}
MLFLOW_DEFAULT_ARTIFACT_ROOT=s3://${MLFLOW_MINIO_BUCKET}/
MLFLOW_S3_ENDPOINT_URL=http://${MLFLOW_MINIO_HOST}:${MLFLOW_MINIO_PORT}
MLFLOW_HOST=0.0.0.0
MLFLOW_PORT=5000
MLFLOW_VERSION=v3.4.0

# MLflow - Auth Config Path (inside container)
MLFLOW_AUTH_CONFIG_PATH=/mlflow/mlflow_auth_config.ini

# MLflow - AWS Configuration (for Boto3)
# Note: Using MinIO (S3-compatible) for artifact storage, but this variable is kept for compatibility
MLFLOW_AWS_DEFAULT_REGION=us-east-1

# MLflow Authentication
MLFLOW_FLASK_SERVER_SECRET_KEY=change_me_with_a_secure_key
MLFLOW_ADMIN_USERNAME=admin
MLFLOW_ADMIN_PASSWORD=change_me_on_first_login

# Langfuse - Authentication & Security
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=change_me_with_a_secure_secret
SALT=change_me_with_a_secure_salt
ENCRYPTION_KEY=change_me_with_64_char_hex_key_generate_via_openssl_rand_hex_32

# Langfuse - PostgreSQL
LANGFUSE_POSTGRES_VERSION=17
LANGFUSE_POSTGRES_USER=langfuse
LANGFUSE_POSTGRES_PASSWORD=change_me_langfuse_password
LANGFUSE_POSTGRES_DB=langfuse
LANGFUSE_DATABASE_URL=postgresql://${LANGFUSE_POSTGRES_USER}:${LANGFUSE_POSTGRES_PASSWORD}@langfuse-postgres:5432/${LANGFUSE_POSTGRES_DB}

# Langfuse - ClickHouse
LANGFUSE_CLICKHOUSE_USER=langfuse_clickhouse
LANGFUSE_CLICKHOUSE_PASSWORD=change_me_clickhouse_password
LANGFUSE_CLICKHOUSE_MIGRATION_URL=clickhouse://langfuse-clickhouse:9000
LANGFUSE_CLICKHOUSE_URL=http://langfuse-clickhouse:8123
LANGFUSE_CLICKHOUSE_CLUSTER_ENABLED=false

# Langfuse - MinIO (S3-compatible storage)
LANGFUSE_MINIO_ROOT_USER=langfuse_minio
LANGFUSE_MINIO_ROOT_PASSWORD=change_me_minio_password
LANGFUSE_MINIO_HOST=langfuse-minio
LANGFUSE_MINIO_PORT=9000
LANGFUSE_MINIO_BUCKET=langfuse

# Langfuse - Redis
LANGFUSE_REDIS_HOST=langfuse-redis
LANGFUSE_REDIS_PORT=6379
LANGFUSE_REDIS_AUTH=change_me_redis_password
LANGFUSE_REDIS_TLS_ENABLED=false

# Langfuse - S3 Configuration
LANGFUSE_S3_EVENT_UPLOAD_BUCKET=langfuse
LANGFUSE_S3_EVENT_UPLOAD_REGION=auto
LANGFUSE_S3_EVENT_UPLOAD_ACCESS_KEY_ID=${LANGFUSE_MINIO_ROOT_USER}
LANGFUSE_S3_EVENT_UPLOAD_SECRET_ACCESS_KEY=${LANGFUSE_MINIO_ROOT_PASSWORD}
LANGFUSE_S3_EVENT_UPLOAD_ENDPOINT=http://${LANGFUSE_MINIO_HOST}:${LANGFUSE_MINIO_PORT}
LANGFUSE_S3_EVENT_UPLOAD_FORCE_PATH_STYLE=true
LANGFUSE_S3_EVENT_UPLOAD_PREFIX=events/

LANGFUSE_S3_MEDIA_UPLOAD_BUCKET=langfuse
LANGFUSE_S3_MEDIA_UPLOAD_REGION=auto
LANGFUSE_S3_MEDIA_UPLOAD_ACCESS_KEY_ID=${LANGFUSE_MINIO_ROOT_USER}
LANGFUSE_S3_MEDIA_UPLOAD_SECRET_ACCESS_KEY=${LANGFUSE_MINIO_ROOT_PASSWORD}
LANGFUSE_S3_MEDIA_UPLOAD_ENDPOINT=http://localhost:9090
LANGFUSE_S3_MEDIA_UPLOAD_FORCE_PATH_STYLE=true
LANGFUSE_S3_MEDIA_UPLOAD_PREFIX=media/

# Langfuse - Batch Export (optional)
LANGFUSE_S3_BATCH_EXPORT_ENABLED=false
LANGFUSE_S3_BATCH_EXPORT_BUCKET=langfuse
LANGFUSE_S3_BATCH_EXPORT_PREFIX=exports/
LANGFUSE_S3_BATCH_EXPORT_REGION=auto
LANGFUSE_S3_BATCH_EXPORT_ENDPOINT=http://${LANGFUSE_MINIO_HOST}:${LANGFUSE_MINIO_PORT}
LANGFUSE_S3_BATCH_EXPORT_EXTERNAL_ENDPOINT=http://localhost:9090
LANGFUSE_S3_BATCH_EXPORT_ACCESS_KEY_ID=${LANGFUSE_MINIO_ROOT_USER}
LANGFUSE_S3_BATCH_EXPORT_SECRET_ACCESS_KEY=${LANGFUSE_MINIO_ROOT_PASSWORD}
LANGFUSE_S3_BATCH_EXPORT_FORCE_PATH_STYLE=true

# Langfuse - Organization & Project Initialization (optional)
LANGFUSE_INIT_ORG_ID=
LANGFUSE_INIT_ORG_NAME=
LANGFUSE_INIT_PROJECT_ID=
LANGFUSE_INIT_PROJECT_NAME=
LANGFUSE_INIT_PROJECT_PUBLIC_KEY=
LANGFUSE_INIT_PROJECT_SECRET_KEY=
LANGFUSE_INIT_USER_EMAIL=
LANGFUSE_INIT_USER_NAME=
LANGFUSE_INIT_USER_PASSWORD=

# Langfuse - Features & Performance
TELEMETRY_ENABLED=true
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true
LANGFUSE_USE_AZURE_BLOB=false
LANGFUSE_INGESTION_QUEUE_DELAY_MS=
LANGFUSE_INGESTION_CLICKHOUSE_WRITE_INTERVAL_MS=

# Email Configuration (optional)
EMAIL_FROM_ADDRESS=
SMTP_CONNECTION_URL=
